{
  "name": "fleet-api",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:local": "prisma generate --schema=./src/prisma/schema.local.prisma && ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "npx prisma generate --schema=./src/prisma/schema.prisma && tsc",
    "start": "node dist/server.js",
    "prisma:generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:generate:local": "prisma generate --schema=./src/prisma/schema.local.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./src/prisma/schema.prisma --name init",
    "prisma:migrate:local": "prisma migrate dev --schema=./src/prisma/schema.local.prisma --name init",
    "seed": "ts-node src/prisma/seed.ts",
    "migrate:deploy": "prisma migrate deploy --schema=./src/prisma/schema.prisma",
    "postinstall": "prisma generate --schema=./src/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@types/express-rate-limit": "^5.1.3",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.0.0",
    "prisma": "^5.6.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
